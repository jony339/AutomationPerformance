        <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor" enabled="false">
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">// Obtener las variables del CSV
String cookieName = vars.get(&quot;CookieName&quot;);
String cookieValue = vars.get(&quot;CookieValue&quot;);
String domain = vars.get(&quot;Domain&quot;);
String path = vars.get(&quot;Path&quot;);
String maxAge = vars.get(&quot;MaxAge&quot;);
String secure = vars.get(&quot;Secure&quot;);

// Validar y parsear MaxAge
int parsedMaxAge;
try {
    parsedMaxAge = Integer.parseInt(maxAge);
} catch (NumberFormatException e) {
    parsedMaxAge = 999999;  // Valor predeterminado si MaxAge no es un número válido
}

// Agregar la cookie al HTTP Cookie Manager
Cookie cookie = new Cookie(cookieName, cookieValue, domain, path, parsedMaxAge, Boolean.parseBoolean(secure));
CookieManager manager = sampler.getCookieManager();

if (manager != null) {
    manager.add(cookie);
} else {
    log.error(&quot;No CookieManager found in sampler&quot;);
}
</stringProp>
        </BeanShellPreProcessor>
